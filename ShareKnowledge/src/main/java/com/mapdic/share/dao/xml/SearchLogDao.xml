<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--必须与接口同名-->
<mapper namespace="com.mapdic.share.dao.SearchLogDao">
    <resultMap id="SearchLogMap" type="com.mapdic.share.model.SearchLog">
        <result property="id" column="id"/>
        <result column="ip" property="ip"/>
        <result column="visitkeyword" property="visitKeyword"/>
        <result column="visitime" property="visitTime"/>
    </resultMap>

    <!--<select id="" resultMap="">-->

    <!--</select>-->

        <insert id="addSearchLog" useGeneratedKeys="true" keyProperty="id" >
            INSERT INTO searchlog(ip,visitkeyword,visitime)
            VALUES (
            #{ip}, #{visitKeyword}, #{visitTime}
            )
        </insert>

</mapper>